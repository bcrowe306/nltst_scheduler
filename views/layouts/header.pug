header.navbar.sticky-top.bg-dark.flex-md-nowrap.p-0.shadow(data-bs-theme='dark')
  a.navbar-brand.col-md-3.col-lg-2.me-0.px-3.fs-6.text-white(href='#') Company name
  ul.navbar-nav.flex-row.d-md-none
    li.nav-item.text-nowrap
      button.nav-link.px-3.text-white(type='button' data-bs-toggle='collapse' data-bs-target='#navbarSearch' aria-controls='navbarSearch' aria-expanded='false' aria-label='Toggle search')
        svg.bi(aria-hidden='true')
          use(xlink:href='#search')
    li.nav-item.text-nowrap
      button.nav-link.px-3.text-white(type='button' data-bs-toggle='offcanvas' data-bs-target='#sidebarMenu' aria-controls='sidebarMenu' aria-expanded='false' aria-label='Toggle navigation')
        svg.bi(aria-hidden='true')
          use(xlink:href='#list')
  #navbarSearch.navbar-search.w-100.collapse
    input.form-control.w-100.rounded-0.border-0(type='text' placeholder='Search' aria-label='Search')